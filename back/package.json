{
  "name": "back",
  "version": "1.0.0",
  "description": "Backend for manual count",
  "main": "dist/index.js",
  "repository": "https://github.com/12urenloop/manual-count-2",
  "author": "Zeus WPI",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "ts-node src/Main.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "dist"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "sqlite3": "^4.0.6",
    "typeorm": "^0.2.16",
    "winston": "^3.2.1"
  }
}
