{
  "name": "back",
  "version": "1.0.0",
  "description": "Backend for manual count",
  "main": "dist/index.js",
  "repository": "https://github.com/12urenloop/manual-count-2",
  "author": "Zeus WPI",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "yarn run build && node dist/index.js",
    "seed": "ts-node scripts/seed.ts",
    "db": "sequelize",
    "clean": "rm -r dist"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "dist"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/sequelize": "^4.27.46",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "mariadb": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "4.27",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.9",
    "sqlite3": "^4.0.6",
    "winston": "^3.2.1"
  }
}
