{
  "name": "back",
  "version": "1.0.0",
  "description": "Backend for manual count",
  "main": "dist/index.js",
  "repository": "https://github.com/12urenloop/manual-count-2",
  "author": "Zeus WPI",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "ts-node src/index.ts",
    "db": "sequelize"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "dist"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/sequelize": "^4.27.46",
    "nodemon": "^1.18.10",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.2.9",
    "sequelize-typescript": "^0.6.9",
    "winston": "^3.2.1"
  }
}
